{
    "name": "watch-in-chrome",
    "displayName": "Watch in Chrome",
    "description": "",
    "version": "0.0.1",
    "publisher": "sneezry",
    "engines": {
        "vscode": "^1.22.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Sneezry/vscode-watch-in-chrome.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-watchinchrome.watch"
    ],
    "main": "./out/extension",
    "logo": "watch-in-chrome.png",
    "contributes": {
        "commands": [
            {
                "command": "vscode-watchinchrome.watch",
                "title": "Watch in Chrome"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/express": "^4.11.1",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/ws": "^4.0.2",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "express": "^4.16.3",
        "ws": "^5.1.1"
    }
}
